msgstr "<big><b>Nepodařilo se spustit příkaz \"%s\".</b></big>\n\n%s"

#: ../panel-plugin/pulseaudio-dialog.c:138
#: ../panel-plugin/pulseaudio-menu.c:261
msgid "Error"
msgstr "Chyba"

#: ../panel-plugin/pulseaudio-button.c:309
#: ../panel-plugin/pulseaudio-notify.c:196
